{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}
<div class="container-fluid">
	<div class="form-group row">
		<div class="col-lg-5">{{ form.first_name }}</div>
		<div class="col-lg-5">{{ form.last_name }}</div>
		<div class="col-lg-2">{{ form.dni }}</div>
	</div>

	<div class="form-group row">
		<div class="col-lg-6">{{ form.gender }}</div>
		<div class="col-lg-6">{{ form.term_cond }}</div>
		<div class="col-lg-6">{{ form.field_date }}</div>
		<div class="col-lg-6">{{ form.email }}</div>
		<div class="col-lg-6">{{ form.my_file }}</div>
		<div class="col-lg-6">{{ form.profile_image }}</div>
		<div class="col-lg-6">{{ form.examples_link }}</div>
		<div class="col-lg-6">{{ form.example_select }}</div>
		<div class="col-lg-6">{{ form.pass }}</div>
		<div class="col-lg-6">{{ form.text }}</div>
	</div>

	<div class="row">
		<div class="col-sm-12">{{ form.submit }}</div>
	</div>

	{{ form.meses }}
	{# Don't forget to add printing form special info at the end.
		    Without this data form will not working propertly.
		    #}
	{{ form.form_build_id }}
	{{ form.form_token }}
	{{ form.form_id }}
</div>
