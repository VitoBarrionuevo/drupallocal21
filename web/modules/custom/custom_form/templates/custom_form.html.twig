<form action="/afip_form/save" method="POST">
	<fieldset>
		<legend>Informacion Personal:</legend>
		<label for="nombre">Nombre:</label>
		<input type="text" id="nombre" name="nombre" value="">
		<label for="apellido">Apellido:</label>
		<input type="text" id="apellido" name="apellido" value="">
		<label for="nacimiento">Fecha de nacimiento:</label>
		<input type="date" id="nacimiento" name="nacimiento">
		<label for="dni">DNI:</label>
		<input type="number" id="dni" name="dni" value="">
		<label for="cuit">CUIT:</label>
		<input type="number" id="cuit" name="cuit" value="">
		<label for="estado civil">Estado Civil:</label>
		<label for="soltero"><input type="radio" id="soltero" name="estado_civil" value="soltero">
			Soltero</label><br>
		<label for="casado"><input type="radio" id="casado" name="estado_civil" value="casado">
			Casado</label><br>
		<label for="hijos">Hijos:</label>
		<input type="number" id="hijos" name="hijos" value="">
	</fieldset>
	<fieldset>
		<legend>Genero:</legend>
		<label for="male"><input class="radio_gender" type="radio" id="male" name="genero" value="male">
			Masculino</label>
		<label for="female"><input type="radio" id="female" name="genero" value="female">
			Femenino</label>
		<label for="other"><input type="radio" id="other" name="genero" value="other">
			Otro</label>

		<div id="especificar">
			<label for="otro_genero">Especifique cual:</label>
			<input type="text" id="otro_genero" name="otro_genero" value="">
		</div>
	</fieldset>
	<fieldset>
		<legend>Direccion:</legend>
		<div id="dropdown">
			<label for="pais">Pais:</label>
			<select id="pais" name="pais"></select>
		</div>
		<div class="ocultar">
		<label for="provincia">Provincia:</label>
		<select id="provincia" name="provincia"></select>
		</div>

		<label for="localidad">Localidad:</label>
		<input type="text" id="localidad" name="localidad" value="">
		<label for="calle">Calle:</label>
		<input type="text" id="calle" name="calle" value="">
		<label for="numero">Numero:</label>
		<input type="number" id="numero" name="numero" value="">
		<label for="piso">Piso:</label>
		<input type="number" id="piso" name="piso" value="">
		<label for="codigo postal">Codigo Postal:</label>
		<input type="number" id="codigo_postal" name="codigo_postal" value="">
	</fieldset>
	<fieldset>
		<legend>Datos de contacto:</legend>
		<label for="email">E-mail:</label>
		<input type="email" id="email" name="email">
		<label for="telefono celular">Telefono celular:</label>
		<input type="number" id="telefono_celular" name="telefono_celular" value="">
		<label for="telefono fijo">Telefono fijo:</label>
		<input type="number" id="telefono_fijo" name="telefono_fijo" value="">
	</fieldset>
	<fieldset>
		<fieldset>
			<label for="user">Usuario:</label>
			<input type="text" id="user" name="user" value="">
			<label for="password">Contrase単a:</label>
			<input type="password" id="password" name="password">
		</fieldset>
		<fieldset>
			<label for="not1"><input type="checkbox" id="not1" name="recibir_notificaciones">
				多Desea recibir notificaciones?:</label><br>
			<label for="not2"><input type="checkbox" id="not2" name="term_cont">
				多Acepta los ternimos y condiciones?:</label><br>
		</fieldset>
		<fieldset>
			<label for="comentarios">多Desea realizar algun comentario?:</label>
			<textarea name="comentarios" rows="5" cols="40"></textarea>
		</fieldset>
		<input type="submit" value="Submit">
	</fieldset>
</form>
<div style="display:none" id="pais_prov">
	{% for pais_prov in variables.pais_prov %}
		<li pais-id="{{pais_prov.pais_id}}" pais-name="{{pais_prov.pais_name}}" provincia-id="{{pais_prov.provincia_id}}" provincia-name="{{pais_prov.provincia_name}}">
	{% endfor %}
</div>
