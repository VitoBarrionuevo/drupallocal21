{#
/**
 * @file profile_entity.html.twig
 * Default theme implementation to present Profile entity data.
 *
 * This template is used when viewing Profile entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_profile_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('profile_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
